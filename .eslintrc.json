{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react"],
  "rules": {
    "semi": "off",
    "react/react-in-jsx-scope": "off",
    "linebreak-style": "off",
    "react/prop-types": "off",
    "object-curly-spacing": "off",
    "comma-dangle": "off",
    "indent": "off",
    "operator-linebreak": "off",
    "max-len": "off",
    "no-unused-vars": "off"
  }
}
